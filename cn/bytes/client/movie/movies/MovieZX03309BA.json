        ]
    },
    {
        "Id": 40,
        "ActionId": 40,
        "NextActionId": 0,
        "Type": 301,
        "IsBlock": 0,
        "IsEnd": 0,
        "BeginDelay": 0.0,
        "EndDelay": 0.0,
        "Params": [
